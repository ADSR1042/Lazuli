{
  "manifest_version": 3,
  "name": "CourseForge",
  "version": "1.0",
  "description": "test",
  "author": "ADSR1042",
  "icons": {
    "48": "icon.png",
    "128": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://zdbk.zju.edu.cn/jwglxt/*",
        "http://zdbk.zju.edu.cn:8080/jwglxt/*",
        "http://chalaoshi.de/*",
        "http://chalaoshi-de-s.webvpn.zju.edu.cn:8001/*"
      ],
      "js": [
        "lib/js/jquery.min.js",
        "js/popup.js",
        "js/content-script.js"
      ],
      "css": [
        "lib/css/inject.css"
      ],
      "run_at": "document_end"
    }
  ],
  "background": {
    "service_worker": "js/background.js"
  },
  "permissions": [
    "notifications",
    "tabs",
    "contextMenus",
    "cookies",
    "webRequest",
    "storage"
  ],
  "action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_security_policy": {},
  "host_permissions": [
    "http://zdbk.zju.edu.cn/*",
    "http://zdbk.zju.edu.cn:8080/*",
    "http://chalaoshi.de/*",
    "http://chalaoshi-de-s.webvpn.zju.edu.cn:8001/*"
  ],
  "externally_connectable": {
    "matches": ["*://zdbk.zju.edu.cn/*"]
  },
  "web_accessible_resources": [
    {
      "resources": [ "data/default.json" ],
      "matches": [ "http://zdbk.zju.edu.cn/*" ]
    }
  ]
}